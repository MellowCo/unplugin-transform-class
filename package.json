{
  "name": "unplugin-transform-class",
  "version": "0.3.0",
  "description": "transform class by rule, support use in vite, rollup, webpack",
  "license": "MIT",
  "homepage": "https://github.com/MellowCo/unplugin-transform-class#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/MellowCo/unplugin-transform-class"
  },
  "bugs": {
    "url": "https://github.com/MellowCo/unplugin-transform-class/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "we",
    "class"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./utils": {
      "require": "./dist/utils.js",
      "import": "./dist/utils.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "uniapp2": "npm run build && npm -C examples/uniapp_vue2 run dev:mp-weixin",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@meoc/utils": "^0.2.3",
    "@rollup/pluginutils": "^5.0.2",
    "unplugin": "1.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.38.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "nodemon": "^2.0.22",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.29.8",
    "webpack": "^5.78.0"
  }
}
